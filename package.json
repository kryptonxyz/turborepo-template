  },
  "devDependencies": {
    "eslint": "7.32.0",
    "eslint-config-custom": "workspace:*",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "prettier": "^2.5.1",
    "turbo": "latest"
  }
}
